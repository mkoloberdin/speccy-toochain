EXE=mktrd trdext cformat bin2hob fmttxt mctrd

all: $(EXE)

mktrd:
	$(CC) -O0 mktrd.cpp
	$(LD) -static -o mktrd mktrd.o

mctrd:
	$(CC) -O0 -o mctrd.o mctrd.c
	$(LD) -static -o mctrd mctrd.o

trdext:
	cp -f trdext.src trdext

cformat:
	cp -f cformat.src cformat

bin2hob:
	cp -f bin2hob.src bin2hob

fmttxt:
	cp -f fmttxt.src fmttxt

clean:
	rm -f *.o $(EXE)

install:
	@for i in  $(EXE) ; do \
	echo "Installing: "$$i ; \
	cp -f $$i $(BINDIR) ; \
	done
